{
  "name": "riseconsulting-test-case",
  "version": "1.0.0",
  "author": "Mustafa Keskin",
  "license": "MIT",
  "private": true,
  "scripts": {
    "api": "node server/api.js",
    "dev": "concurrently \"next dev\" \"tailwindcss -i src/styles/globals.scss -o public/styles/main.css -w\"",
    "build": "tailwindcss -m -i src/styles/globals.scss -o public/main.css && next build",
    "start": "next start",
    "fire": "concurrently \"npm run dev\" \"npm run api\"",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "concurrently": "^7.6.0",
    "eslint-config-next": "^13.1.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next": "^13.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "clsx": "^1.2.1",
    "store": "^2.0.12",
    "tailwindcss": "^3.2.4",
    "wretch": "^2.3.2",
    "zustand": "^4.3.2"
  }
}
