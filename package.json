{
  "name": "riseconsulting-test-case",
  "version": "1.0.0",
  "author": "Mustafa Keskin",
  "private": true,
  "scripts": {
    "api": "node server/index.js",
    "dev": "concurrently \"next dev\" \"npm run tailwind -- --watch\" \"npm run api\"",
    "build": "concurrently \"next build\" \"npm run tailwind\"",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "tailwind": "tailwindcss -i src/styles/globals.scss -o public/styles/main.css"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "store": "^2.0.12",
    "tailwindcss": "^3.2.4",
    "wretch": "^2.3.2",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@types/store": "^2.0.2",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.14",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next": "^13.1.2",
    "postcss-nesting": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  }
}
